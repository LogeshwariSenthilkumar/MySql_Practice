use collegedb;

CREATE TABLE Student (
    student_id INT PRIMARY KEY,
    student_name VARCHAR(50),
    age INT,
    branch VARCHAR(30),
    email VARCHAR(50),
    marks DECIMAL(5,2),
    balance DECIMAL(10,2),
    dob DATE
);

