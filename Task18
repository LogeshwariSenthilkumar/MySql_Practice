select * from student;

select student_name,branch from student;
select distinct(branch) from student;
select * from student where age >20;
select * from student where student_name like 'l%';
select * from student where student_id in (1,3,5);
select * from student where age between 20 and 25;
select * from student order by age desc;
select count(student_id) from student;
select max(age) from student;
select min(age) from student;
select avg(age) from student;
select branch,count(student_id) from student group by branch having count(student_id)>2;

