//1.select:
//11.all columns:
select * from Employee;

//1.2.particular columns:
select id,name from Employee;

//1.3.all columns with particular record:
select * from Employee where id=3;

//1.4.particular column particular record:
select id,name from Employee where id=4;

//2.alias:
select id as roll from Employee;


//3.Distinct:
select distinct(name) from Employee;


