//1.Operator:
//1.1 Arithmetic Operator:

select 3+3;
select 6-5;
select 3*2;
select 4/2;
select 10%2;

//1.2 comparsion Operator:

select * from employee where salary>1000000;
select * from employee where salary>=1000000;
select * from employee where salary<1000000;
select * from employee where salary<=1000000;
select * from employee where salary=1000000;
select * from employee where salary<>1000000;

//1.3 special Operator:

//and:
select * from employee where salary<2000000 and salary>1000000;

//or:
select * from employee where salary<2000000 or salary>1000000;

//not:
select * from employee where not salary=1000000;

//between:
select * from employee where salary between 1000000 and 2000000;


//not between:
select * from employee where salary not between 1000000 and 2000000;

//is null:
select * from employee where job_desc is null;


//is not null:
select * from employee where job_desc is not null;


//like:
select * from employee where ename like 'r%';
select * from employee where ename like '%a';
select * from employee where ename like '___i%';
select * from employee where ename like '%a%';

//not like:
select * from employee where ename not like 'r%';
select * from employee where ename not like '%a';
select * from employee where ename not like '___i%';
select * from employee where ename not like '%a%';

//in:
select * from employee where job_desc in("CEO","Manager");

//not in:
select * from employee where job_desc not in("CEO","Manager");




